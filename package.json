    "v-viewer": "^1.5.1",
    "vdjs": "^1.0.0",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-count-to": "^1.0.13",
    "vue-cute-timeline": "^1.2.8",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.6",
    "vue-seamless-scroll": "^1.1.23",
